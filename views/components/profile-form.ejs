<main class="h-full pb-16 overflow-y-auto">
  <div class="container px-6 mx-auto grid">

    <h2 class="my-6 text-2xl font-semibold text-gray-700 dark:text-gray-200">
      Update Your Profile
    </h2>

    <div class="px-4 py-3 mb-8 bg-white rounded-lg shadow-md dark:bg-gray-800">
      <form name="edit-profile-form">

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Name</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="Jane Doe" type="text" name="name" id="name"
            />
          </label>
        </div>

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Email</span>
            <input readonly
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="JaneDoe@gmail.com" type="text" name="email" id="email"
            />
          </label>
        </div>

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Phone Number</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="0123456678" type="number" id="number" name="number"
            />
          </label>
        </div>

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Secondary Email</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="secondary@email.com" type="email" name="secondaryEmail" id="secondaryEmail"
            />
          </label>
        </div>

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Address</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="No 32 your house address" type="text" name="address" id="address"
            />
          </label>
        </div>

        <div class="mt-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">State or Province</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="Washinton DC" type="text" name="state" id="state"
            />
          </label>
        </div>

        <div class="mt-4 mb-4">
          <label class="block text-sm my-2">
            <span class="text-gray-700 dark:text-gray-400">Country</span>
            <input
              class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
              placeholder="Nigeria" type="text" name="country" id="country"
            />
          </label>
        </div>

        <div class="flex flex-col flex-wrap mt-4 space-y-4 md:flex-row md:items-end md:space-x-4">
          <button class="px-10 py-4 font-medium leading-5 text-white transition-colors duration-150 bg-purple-600 border border-transparent rounded-lg active:bg-purple-600 hover:bg-purple-700 focus:outline-none focus:shadow-outline-purple" id="submit">
            Submit
          </button>
        </div>

      </form>
    </div>

  </div>
</div>